{
  "info": {
    "name": "Smart Ticket Tracker API",
    "description": "Complete API collection for Smart Ticket Tracker",
    "version": "1.0.0"
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\",\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"phone\": \"+1234567890\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "register"]
            }
          }
        },
        {
          "name": "Login User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "login"]
            }
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"{{refreshToken}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/refresh",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "refresh"]
            }
          }
        }
      ]
    },
    {
      "name": "Routes",
      "item": [
        {
          "name": "Get All Routes",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/routes",
              "host": ["{{baseUrl}}"],
              "path": ["api", "routes"]
            }
          }
        },
        {
          "name": "Get Route by ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/routes/{{routeId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "routes", "{{routeId}}"]
            }
          }
        },
        {
          "name": "Search Routes",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/routes/search?from=Central&to=Airport",
              "host": ["{{baseUrl}}"],
              "path": ["api", "routes", "search"],
              "query": [
                {
                  "key": "from",
                  "value": "Central"
                },
                {
                  "key": "to",
                  "value": "Airport"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Schedules",
      "item": [
        {
          "name": "Get Schedules by Route",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/schedules?routeId={{routeId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "schedules"],
              "query": [
                {
                  "key": "routeId",
                  "value": "{{routeId}}"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Bookings",
      "item": [
        {
          "name": "Get User Bookings",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/bookings",
              "host": ["{{baseUrl}}"],
              "path": ["api", "bookings"]
            }
          }
        },
        {
          "name": "Create Booking",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"routeId\": \"{{routeId}}\",\n  \"scheduleId\": \"{{scheduleId}}\",\n  \"travelDate\": \"2024-01-15\",\n  \"seatNumber\": \"A1\",\n  \"fare\": 150\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/bookings",
              "host": ["{{baseUrl}}"],
              "path": ["api", "bookings"]
            }
          }
        },
        {
          "name": "Cancel Booking",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"reason\": \"Change of plans\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/bookings/{{bookingId}}/cancel",
              "host": ["{{baseUrl}}"],
              "path": ["api", "bookings", "{{bookingId}}", "cancel"]
            }
          }
        }
      ]
    },
    {
      "name": "Forecast",
      "item": [
        {
          "name": "Get Prediction",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{forecastUrl}}/predict?route=Central Station to Airport Express&date=2024-01-15&hour=9",
              "host": ["{{forecastUrl}}"],
              "path": ["predict"],
              "query": [
                {
                  "key": "route",
                  "value": "Central Station to Airport Express"
                },
                {
                  "key": "date",
                  "value": "2024-01-15"
                },
                {
                  "key": "hour",
                  "value": "9"
                }
              ]
            }
          }
        },
        {
          "name": "Get Route Analytics",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{forecastUrl}}/routes/{{routeId}}/analytics",
              "host": ["{{forecastUrl}}"],
              "path": ["routes", "{{routeId}}", "analytics"]
            }
          }
        }
      ]
    },
    {
      "name": "Health",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/health",
              "host": ["{{baseUrl}}"],
              "path": ["api", "health"]
            }
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5000"
    },
    {
      "key": "forecastUrl",
      "value": "http://localhost:5001"
    },
    {
      "key": "accessToken",
      "value": ""
    },
    {
      "key": "refreshToken",
      "value": ""
    },
    {
      "key": "routeId",
      "value": ""
    },
    {
      "key": "scheduleId",
      "value": ""
    },
    {
      "key": "bookingId",
      "value": ""
    }
  ]
}
